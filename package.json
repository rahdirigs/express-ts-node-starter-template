{
  "name": "tsnode-express-server-template",
  "version": "1.0.0",
  "description": "Express TS Node server starting template",
  "main": "src/server.ts",
  "scripts": {
    "build": "NODE_ENV=production tsc --project ./",
    "start": "node dist/server.js",
    "watch": "nodemon --exec ts-node --esm src/server.ts",
    "server": "NODE_ENV=development npm run watch"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "author": {
    "name": "Giridhar Shanbhag",
    "email": "rahdirigs@gmail.com"
  }
}